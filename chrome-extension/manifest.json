{
  "manifest_version": 3,
  "name": "Wizzo Magic Wand",
  "version": "2.0",
  "description": "Snooze tabs and chat with your magic Wizzo assistant",
  "side_panel": {
    "default_path": "sidepanel/index.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "alarms",
    "sidePanel"
  ],
  "action": {
    "default_popup": "popup/index.html"
  },
  "background": {
    "service_worker": "background/main.js"
  }
}
